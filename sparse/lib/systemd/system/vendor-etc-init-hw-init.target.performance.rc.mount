[Unit]
Description=Droid mount for /vendor/etc/init/hw/init.target.performance.rc
Before=local-fs.target

[Mount]
What=/usr/libexec/droid-hybris/vendor/etc/init/hw/init.target.performance.rc
Where=/vendor/etc/init/hw/init.target.performance.rc
Type=none
Options=ro,bind
TimeoutSec=5

[Install]
WantedBy=local-fs.target
